{
	"name": "vs-bem-generate",
	"displayName": "vs-bem-generate",
	"description": "Plugin for generating BEM-files",
	"version": "0.7.0",
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.51.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.yandex-team.ru/pelican/vs-bem-generate"
	},
	"publisher": "vichkunina",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vs-bem-generate.bemGenerate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vs-bem-generate.bemGenerate",
				"title": "Bem generate"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder",
					"command": "vs-bem-generate.bemGenerate"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.51.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"bemg": "1.3.0"
	}
}
